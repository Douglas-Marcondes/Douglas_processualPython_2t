nome = input("Digite seu nome: ")
print (f"Seu nome é: {nome}")
idade = input("Digite sua idade: ")
print (f"sua idade é: {idade}")
escola = input("Qual é a sua escola? ")
print (f"Sua escola é: {escola}")
profissao = input("Com o que você trabalha? ")
print (f"Sua profissão é: {profissao}")
estudo = input("Qual ano você está? ")
print (f"Seu ano é: {estudo}")
